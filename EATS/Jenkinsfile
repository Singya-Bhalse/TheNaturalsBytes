pipeline {
    agent any
    tools{
        maven 'maven_3_5_0'
    }
    stages{
        stage('Build Maven'){
            steps{
               checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: '71eb0d90-c3af-4543-afca-4ea0934521ba', url: 'https://github.com/singyabhalse/TheNaturalsBytes.git']])
                sh 'mvn clean install'
            }
        }
 
    }
}