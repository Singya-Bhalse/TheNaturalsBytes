pipeline {
    agent any
    tools{
        maven 'maven_3_5_0'
    }
    stages{
        stage('Build Maven'){
            steps{
              checkout scmGit(branches: [[name: 'https://github.com/singyabhalse/TheNaturalsBytes/tree/JenkensDemo/EATS']] url: 'https://github.com/singyabhalse/TheNaturalsBytes.git']])
                sh 'mvn clean install'
            }
        }
 
    }
}
